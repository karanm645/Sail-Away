<h3><%= "Create New Item" %></h3>
<p><%= button_to "New Item", "/dashboard/items/new", method: :get %></p>

<h3><%= "All Items" %></h3>

<% @items.each do |item| %>
  <h3>name: <%= item.name %></h3> <%= link_to "Delete #{item.name}", "/dashboard/items/#{item.id}", method: :delete %>
  <p>category: <%= item.category %></p>
  <p>quantity: <%= item.quantity %></p>
<% end  %>