<h1> Items Page </h1>
<p><%= button_to "Create New Item", "/dashboard/items/new", method: :get %></p>

<h2><%= "All Items" %></h2>

<% @items.each do |item| %>
  <h3><%= item.name %></h3> 
  <p>category: <%= item.category %></p>
  <p>quantity: <%= item.quantity %></p>
  <p><%= link_to "Delete #{item.name}", "/dashboard/items/#{item.id}", method: :delete %> | <%= link_to "Update #{item.name}", "/dashboard/items/#{item.id}/edit" %></p>
<% end  %>