<%= form_with url: "/dashboard/items/#{@item.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :category %>
  <%= form.text_field :category %>
  <%= form.label :quantity %>
  <%= form.text_field :quantity %>
  <%= form.submit "Update Item" %>
<% end %>
